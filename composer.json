{
    "name": "josepostiga/skeleton-php-package",
    "description": "A PHP package skeleton",
    "keywords": ["php", "skeleton", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Postia",
            "email": "jose.postiga@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "JosePostiga\\SkeletonPhpPackge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
